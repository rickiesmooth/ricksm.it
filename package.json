{
  "name": "website-new",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "cypress run --config-file cypress/cypress.json",
    "cypress:open": "cypress open --config-file cypress/cypress.json",
    "build": "NODE_ENV=production webpack",
    "watch": "NODE_ENV=development API_URL=http://localhost:3000/ webpack -w",
    "start": "serve build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rickiesmooth/website-new.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rickiesmooth/website-new/issues"
  },
  "homepage": "https://github.com/rickiesmooth/website-new#readme",
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@toycode/markdown-it-class": "^1.2.4",
    "@types/glob": "^7.1.3",
    "@types/node": "^14.14.20",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^5.0.1",
    "ejs-loader": "^0.5.0",
    "gl-matrix": "^3.3.0",
    "glob": "^7.1.6",
    "gray-matter": "^4.0.2",
    "html-webpack-plugin": "^4.5.0",
    "markdown-it": "^12.0.4",
    "mini-css-extract-plugin": "^1.3.3",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^3.0.0",
    "prismjs": "^1.23.0",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "serve": "^11.3.2",
    "style-loader": "^2.0.0",
    "tailwindcss": "^1.9.6",
    "terser-webpack-plugin": "^4.2.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.3.1",
    "workbox-core": "^6.0.2",
    "workbox-precaching": "^6.0.2",
    "workbox-routing": "^6.0.2",
    "workbox-strategies": "^6.0.2",
    "workbox-streams": "^6.0.2",
    "workbox-webpack-plugin": "^6.0.2"
  },
  "babel": {
    "plugins": [
      [
        "prismjs",
        {
          "languages": [
            "javascript",
            "typescript",
            "jsx",
            "tsx",
            "css",
            "html"
          ],
          "plugins": [
            "line-numbers",
            "show-language",
            "diff-highlight"
          ],
          "theme": "solarizedlight",
          "css": true
        }
      ]
    ],
    "presets": [
      [
        "@babel/preset-env",
        {
          "bugfixes": true,
          "targets": {
            "esmodules": true
          }
        }
      ],
      "@babel/preset-typescript"
    ]
  },
  "devDependencies": {
    "babel-plugin-prismjs": "^2.0.1",
    "cypress": "^6.2.0"
  }
}
